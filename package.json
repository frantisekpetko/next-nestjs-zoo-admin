    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fishery": "^2.2.2",
    "graphql-zeus": "^6.0.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^28.1.3",
